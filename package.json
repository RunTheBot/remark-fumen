{
  "name": "remark-fumen",
  "version": "0.0.4",
  "description": "turns fumen data in markdown into images with remark",
  "license": "MIT",
  "repository": "RunTheBot/remark-fumen",
  "type": "module",
  "main": "dist/index.js",
  "author": {
    "name": "RunTheBot",
    "email": "runthebot@gmail.com"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "samples"
  ],
  "scripts": {
    "test": "jest",
    "coverage": "npm test -- --coverage",
    "postcoverage": "open-cli coverage/lcov-report/index.html",
    "lint": "eslint .",
    "flow": "flow check",
    "docs": "documentation build src/index.js -f md -o docs/README.md && documentation build src/** -f html -o docs",
    "postdocs": "git add README.md",
    "clean": "rimraf dist",
    "flowbuild": "flow-copy-source src dist",
    "prebuild": "npm run docs && npm run clean && npm run flowbuild",
    "build": "babel src -d dist",
    "prerelease": "npm run lint && npm test && npm run build",
    "release": "standard-version",
    "postpublish": "git push origin master --follow-tags"
  },
  "husky": {},
  "jest": {
    "verbose": true
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "node",
    "module",
    "package",
    "template",
    "npm"
  ],
  "dependencies": {
    "canvas": "2.11.2",
    "gifencoder": "2.0.1",
    "mdast-util-find-and-replace": "3.0.1",
    "tetris-fumen": "1.1.3"
  },
  "devDependencies": {
    "@babel/cli": "7.23.4",
    "@babel/core": "7.6.2",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/preset-env": "7.6.2",
    "@babel/preset-flow": "7.0.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "documentation": "14.0.2",
    "eslint": "6.5.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-flowtype": "4.3.0",
    "eslint-plugin-flowtype-errors": "4.1.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "27.6.0",
    "eslint-plugin-prettier": "3.1.1",
    "flow-bin": "0.108.0",
    "flow-copy-source": "2.0.8",
    "husky": "3.0.7",
    "jest": "29.7.0",
    "lint-staged": "9.4.0",
    "open-cli": "7.2.0",
    "prettier": "1.18.2",
    "rehype-stringify": "10.0.0",
    "remark": "15.0.1",
    "remark-rehype": "11.0.0",
    "rimraf": "3.0.0",
    "standard-version": "9.5.0"
  }
}
